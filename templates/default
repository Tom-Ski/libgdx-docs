	var userList1 = new List("list1"); 
	var userList2 = new List("list2"); 
	var userList3 = new List("list3"); 

	$('#searchbar').on('keyup', function() {
		var searchString = $(this).val();
		search(searchString, userList1);
		search(searchString, userList2);
		search(searchString, userList3);
	});

</script>
